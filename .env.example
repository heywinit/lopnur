# Solana RPC Provider Configuration
# Copy this file to .env and fill in your API keys
# These environment variables will be used for RPC providers defined in config/providers.json

# Helius RPC Configuration
HELIUS_RPC_ENDPOINT=https://mainnet.helius-rpc.com
HELIUS_API_KEY=YOUR_HELIUS_API_KEY

# QuickNode RPC Configuration
QUICKNODE_RPC_ENDPOINT=https://api.mainnet-beta.solana.com
QUICKNODE_API_KEY=YOUR_QUICKNODE_API_KEY

# Triton RPC Configuration
TRITON_RPC_ENDPOINT=https://triton.rpcpool.com
TRITON_API_KEY=YOUR_TRITON_API_KEY

# Public RPC Configuration
PUBLIC_RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Add your custom providers below following the same pattern:
# PROVIDER_NAME_RPC_ENDPOINT=https://your-provider-endpoint.com
# PROVIDER_NAME_API_KEY=YOUR_API_KEY 